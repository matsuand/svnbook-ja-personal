#. type: Content of: <reference><refentry><refnamediv><refname>
#: ref-svnrdump.xml:275
msgid "svnrdump load"
msgstr ""

#. type: Content of: <reference><refentry><refsynopsisdiv><para>
#: ref-svnrdump.xml:280
msgid "<literal>svnrdump load DEST_URL</literal>"
msgstr ""

#. type: Content of: <reference><refentry><refsect1><para>
#: ref-svnrdump.xml:287
msgid ""
"Read from standard input revision information described in a Subversion "
"repository dump stream, and load that information into the repository "
"located at <replaceable>DEST_URL</replaceable>."
msgstr ""

#. type: Content of: <reference><refentry><refsect1><informalexample><screen>
#: ref-svnrdump.xml:300
#, no-wrap
msgid ""
"<xref linkend=\"svn.ref.svnrdump.sw.config_dir\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.config_option\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.no_auth_cache\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.non_interactive\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.password\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.quiet\" />\n"
"<xref linkend=\"svn.ref.svnrdump.sw.trust_server_cert\" />\n"
